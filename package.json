{
  "name": "analytics-query-language",
  "displayName": "analytics-query-language",
  "description": "VSCode support for analytics query language",
  "version": "0.0.1",
  "publisher": "no-publisher",
  "engines": {
    "vscode": "^1.26.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "scripts": {
    "vscode:prepublish": "cd client && npm run update-vscode && cd .. && npm run compile",
    "watch:client": "cd client && npm run watch",
    "compile:client": "cd client && npm run compile",
    "compile": "npm run compile:client",
    "postinstall": "cd client && npm postinstall"
  },
  "main": "./client/out/extension",
  "activationEvents": [
    "onLanguage:aql"
  ],
  "contributes": {
    "languages": [
      {
        "id": "aql",
        "aliases": [
          "analytics-query-language",
          "aql"
        ],
        "extensions": [
          "aql"
        ],
        "configuration": "./client/syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "aql",
        "scopeName": "aql",
        "path": "./client/syntaxes/aql.tmLanguage.json"
      }
    ]
  }
}
